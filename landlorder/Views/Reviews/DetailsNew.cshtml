@model landlorder.ViewModels.ReviewViewModel
@{
    ViewBag.Title = ViewBag.address + " landlord reviews | landlorder";
}


<div class="review-section">
    <div class="container">
        <div class="row">


            <div class="container">
                <div class="detailsheader">
                    <div class="detailsheading-wrap">
                        <div class="detail-items">

                            <div class="item col-md-12">
                                <div class="content">
                                    <h3 class="logintitle">@ViewBag.address</h3>
                                </div>
                                <span><i class="fa fa-pencil"></i></span>
                            </div><!--//item-->
                            <hr />
                            <div class="detailitem-subitem">
                                <h5 class="logintitle">@ViewBag.address</h5>
                            </div>
                            <div class="detailitem-subitem">
                                <h5 class=" logintitle">
                                    @ViewBag.address
                                </h5>
                            </div>
                            <div class="detailitem-subitem">
                                <h5 class="logintitle">@ViewBag.address</h5>
                            </div>

                        </div>
                    </div>
                </div>
            </div>


                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.formatted_address)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.formatted_address)
                    </dd>
                </dl>
            }

            <p>
                @Html.ActionLink("Write a Review", "CreateNewProperty", new { id = Model.formatted_address })
                @Html.ActionLink("Back to List", "Index")
            </p>

        </div>
    </div>
</div>